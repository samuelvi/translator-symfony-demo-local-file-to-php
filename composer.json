{
  "name": "samuelvi/translator-symfony-demo-local-file-to-php",
  "license": "MIT",
  "type": "project",
  "description": "Spreadsheet Translator. Symfony Demo Application. Tankes a local spreadsheet file and creates a translation file per locale in Php format",
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "classmap": [
      "app/AppKernel.php"
    ]
  },
  "require": {
    "php": ">=8.1",
    "symfony/symfony": "^6.0|7.0.*",
    "samuelvi/spreadsheet-translator-core": "^8.0",
    "samuelvi/spreadsheet-translator-symfony-bundle": "^8.0",
    "samuelvi/spreadsheet-translator-provider-localfile": "^8.0",
    "samuelvi/spreadsheet-translator-reader-xlsx": "8.1.1",
    "samuelvi/spreadsheet-translator-exporter-php": "^8.0"
  },
  "require-dev": {
    "rector/jack": "^0.2.9",
    "rector/rector": "^2.1"
  },
  "config": {
    "bin-dir": "bin"
  },
  "scripts": {
    "post-create-project-cmd": [
      "bash ./bin/create_translations_folder.sh"
    ],
    "post-update-cmd": [
      "bash ./bin/create_translations_folder.sh"
    ],
    "post-install-cmd": [
      "bash ./bin/create_translations_folder.sh"
    ]
  }
}